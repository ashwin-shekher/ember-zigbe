<Header>
  {{input value=searchTerm class="search-box" placeholder="Search By App Name"}}
</Header>
<div class="px-5">
  <div class="heading">
    <h1 class="my-5">
      Daily Top 100
    </h1>
    <button class="btn" type="button" {{action "navigateToAddApp"}}>
      ADD
    </button>
  </div>
  <div class="app-list-container my-5" >

    <AppItems @model={{@model}}  @searchTerm={{this.searchTerm}} />
  </div>
</div>